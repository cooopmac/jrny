{
  "expo": {
    "name": "jrny",
    "slug": "jrny",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "jrny",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.jrny"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Gabarito-Regular.ttf",
            "./assets/fonts/Gabarito-Medium.ttf",
            "./assets/fonts/Gabarito-SemiBold.ttf",
            "./assets/fonts/Gabarito-Bold.ttf",
            "./assets/fonts/Gabarito-Black.ttf",
            "./assets/fonts/Gabarito-ExtraBold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
